def test_basic_operations():
    assert 1 / 2 == 0.5
    assert 1 + 2 - 3 == 0
    assert 1 - 2 - 3 == -4
    assert 1 + 2 == 3
    assert 90 / 2 == 45
    assert 90 / 2 + 10 == 55
    assert 10 + 10 / 10 == 11
    assert (10 + 10) / 10 == 2
    assert 10 * 10 / 20 == 5
    assert 10 * (10 / 20) == 5
    assert 3 ** 2 == 9
    assert 3 ** 3 == 27
    assert 3 ** 3 * 2 == 54
